#include <iostream>
#include <algorithm>
#include <vector>
#include <string>
#include <cstring>
#include <list>
#include <cmath>
using namespace std;

int numbers[100000001];

int main(void)
{
    int value;
    long long int result = 0;
    scanf("%d", &value);
    
    for(int i=1; i<=9; i++) numbers[i] = 1;
    numbers[10] = 2;
    
    if(value > 9) {
        for(int i=11; i<=value; i++) {
            numbers[i] = numbers[i - 1];
            if((i/pow(10, numbers[i - 1] - 1)) >= 10)
            {
                numbers[i] += 1;
            }
        }
    }
    for(int i=0; i<=value; i++) result += numbers[i];
    printf("%lld\n", result);
    return 0;
}

